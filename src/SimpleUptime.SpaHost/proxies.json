{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "api": {
      "matchCondition": {
        "route": "/api/{*restOfPath}"
      },
      "backendUri": "https://%WEBSITE_HOSTNAME%/api/{restOfPath}"
    },
    "app": {
      "matchCondition": {
        "route": "/",
        "methods": [ "GET", "HEAD", "OPTIONS" ]
      },
      "backendUri": "https://simpleuptimeuatdata001.blob.core.windows.net/www/index.html"
    },
    "static": {
      "matchCondition": {
        "route": "/{*restOfPath}",
        "methods": [ "GET", "HEAD", "OPTIONS" ]
      },
      "backendUri": "https://simpleuptimeuatdata001.blob.core.windows.net/www/{restOfPath}"
    }
  }
}